{
  "compilerOptions": {
    "target": "esnext",
    "module": "es2015",
    "incremental": true,
    "removeComments": true,
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "jsx": "react",
    "typeRoots": ["./node_modules/@types"],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],"exclude": [
    "node_modules"
  ]
}